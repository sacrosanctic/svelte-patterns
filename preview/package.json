{
	"name": "svelte-patterns",
	"type": "module",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "pnpm \"/dev:/\"",
		"dev:content": "velite dev --watch",
		"dev:svelte": "pnpm build:search && vite dev",
		"build:contributors": "tsx ./scripts/build-contributors-data.ts",
		"build": "velite && pnpm build:search && vite build",
		"build:search": "tsx ./scripts/update-velite-output.js && tsx ./scripts/build-search-data.js",
		"preview": "vite preview",
		"check": "velite && pnpm build:search && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "pnpm build:content && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check:content": "node scripts/enforce-convention.js"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.1",
		"@svecodocs/kit": "^0.5.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "4.1.17",
		"@types/node": "^24.10.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-oxlint": "^1.31.0",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"mdsx": "^0.0.7",
		"phosphor-svelte": "^3.0.1",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"runed": "^0.37.0",
		"svelte": "^5.45.5",
		"svelte-check": "^4.3.4",
		"tailwindcss": "4.1.17",
		"tsx": "^4.21.0",
		"type-fest": "^5.3.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"velite": "^0.3.0",
		"vite": "^7.2.6"
	}
}
